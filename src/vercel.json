
{
  "version": 2,
  "builds": [
    { "src": "public/src/index.html", 
      "use": "@vercel/static" }
  ],
  "routes": [
    {
  "src": "/images/(.*)",
  "dest": "public/images/$1"
}, 
   {
  "src": "/css/(.*)",
  "dest": "public/css/$1"
}
  ],
  "outputDirectory": [
    {
  "outputDirectory": "public"
} 
  ],
  "rewrites": [
    { "source": "/old-path", "destination": "/new-path" }
  ],
  "redirects": [
    { "source": "/old-path", "destination": "/new-path", "permanent": true }
  ]
}

